package prime;

import java.time.*;
import java.util.List;

import prime.prog.Prime;

public class Main {
	
	public static void main(String[] args) {
		int n = 10000;
		long time;
		long finnish;
		
		for(int i = 1; i <= 7; i++) {
			time = System.currentTimeMillis();
			
			Prime.primeList(n);
			
			finnish = System.currentTimeMillis();
			
			System.out.println("n = " + n + " *** " + "time = " + (int) 1000* (finnish-time) + " milliseconds");
			
		}
		
	}
}
